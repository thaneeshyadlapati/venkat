pipeline {
  agent any
  stages {
    stage('Plan') {
      steps {
        echo 'This is a Planning project'
      }
    }

    stage('Code') {
      steps {
        echo 'This is a Coding project'
      }
    }

    stage('Buld') {
      steps {
        echo 'This is a Building project'
      }
    }

    stage('Test') {
      steps {
        echo 'This is a Testing project'
      }
    }

    stage('Release') {
      steps {
        echo 'This is a Releasing schedule'
      }
    }

    stage('Deploy') {
      steps {
        echo 'There is Deploying done here'
      }
    }

    stage('Operate') {
      steps {
        echo 'Operating the product'
      }
    }

  }
}